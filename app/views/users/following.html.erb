<div class="row">
  <div class="col md-6 offset-3">

    <%= render "/users/profile_navbar", user: @user %>

    
    
    <ul class="list-group">
      <% @user.leaders.each do |leader| %>
        <li class="list-group-item">
          <%= link_to "#{leader.username}", user_path(leader.username) %>
        </li>
      <% end %>
    </ul>
  </div>  
</div>
